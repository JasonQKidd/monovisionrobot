  17          * 输入 : del
  18          * 输出 : 无
  19          ***********************************************************************/
  20          void Delay(uint del)
  21          {
  22   1              uint i,j;
  23   1              for(i=0; i<del; i++)
  24   1              for(j=0; j<1827; j++)   
  25   1              ;
  26   1      }
  27          
  28          /********************************************************************
  29          * 名称 : Main()
  30          * 功能 : 实现灯的闪烁
  31          * 输入 : 无
