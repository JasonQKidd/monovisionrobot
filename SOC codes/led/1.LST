  42   2                      for(i=0; i<7; i++)                      //移位7次
  43   2                      {
  44   3                              P0 = _cror_(P0, 1);             //_crol_()这个函数在intrins.h中定义了，用于循环右移
  45   3                              Delay(15);
  46   3                      }
  47   2                      P0 = 0xfe;
  48   2                      for(i=0; i<7; i++)
  49   2                      {
  50   3                              P0 = _crol_(P0, 1);         //用于循环左移
  51   3                              Delay(15);
  52   3                      }
  53   2      
  54   2              }
  55   1      }
